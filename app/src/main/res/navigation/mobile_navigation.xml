        tools:layout="@layout/fragment_forms3" />

    <fragment
        android:id="@+id/navigation_forms4"
        android:name="br.dev.henry.dailytek.ui.forms.clima_emocional.forms4.FormsFragment4"
        tools:layout="@layout/fragment_forms4" />

    <fragment
        android:id="@+id/navigation_select_forms"
        android:name="br.dev.henry.dailytek.ui.forms.select_forms.SelectFormsFragment"
        tools:layout="@layout/fragment_select_forms" />

</navigation>
